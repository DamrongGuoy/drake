<?xml version="1.0" ?>
<sdf version="1.6" xmlns:drake="drake.mit.edu">
  <model name="paddle">
    <link name="dummy_1">
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.01</mass>
        <inertia>
          <ixx> 0.00001</ixx>
          <ixy> 0.000000</ixy>
          <ixz> 0.0000</ixz>
          <iyy> 0.00001</iyy>
          <iyz> 0.000000</iyz>
          <izz> 0.00001</izz>
        </inertia>
      </inertial>
    </link>    
    <link name="dummy_2">
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.01</mass>
        <inertia>
          <ixx> 0.00001</ixx>
          <ixy> 0.000000</ixy>
          <ixz> 0.0000</ixz>
          <iyy> 0.00001</iyy>
          <iyz> 0.000000</iyz>
          <izz> 0.00001</izz>
        </inertia>
      </inertial>
    </link>        
    <link name="dummy_3">
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <pose>0 0 0 0 0 0</pose>
        <mass>0.01</mass>
        <inertia>
          <ixx> 0.00001</ixx>
          <ixy> 0.000000</ixy>
          <ixz> 0.0000</ixz>
          <iyy> 0.00001</iyy>
          <iyz> 0.000000</iyz>
          <izz> 0.00001</izz>
        </inertia>
      </inertial>
    </link>            
    <link name="paddle">
      <pose>0 0 0 0 0 0</pose>
      <inertial>
        <!-- Assume a density of 250 kg/m^3 -->
        <pose>0 0 0 0 0 0</pose>
        <mass>0.16680778</mass>
        <inertia>
          <ixx> 1.16112052e-03</ixx>
          <ixy> -1.15805286e-20</ixy>
          <ixz> -4.13590306e-22</ixz>
          <iyy> 1.30906051e-04</iyy>
          <iyz> 0.000000</iyz>
          <izz> 1.11409827e-03</izz>
        </inertia>
      </inertial>
      <visual name="paddle_visual">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <drake:declare_convex/>
            <uri>paddle.obj</uri>
            <scale>1.0 1.0 1.0</scale>
          </mesh>
        </geometry>
      </visual>
      <collision name="paddle_collision">
        <pose>0 0 0 0 0 0</pose>
        <geometry>
          <mesh>
            <drake:declare_convex/>
            <uri>paddle.obj</uri>
            <scale>1.0 1.0 1.0</scale>
          </mesh>
        </geometry>
        <drake:proximity_properties>
          <drake:mu_dynamic>1.0</drake:mu_dynamic>
          <drake:mu_static>1.0</drake:mu_static>
          <drake:mesh_resolution_hint>0.05</drake:mesh_resolution_hint>
          <drake:hydroelastic_modulus>3e3</drake:hydroelastic_modulus>
          <drake:hunt_crossley_dissipation>5.0</drake:hunt_crossley_dissipation>
          <drake:compliant_hydroelastic/>
        </drake:proximity_properties>
      </collision>
    </link>
    <joint name="x_joint" type="prismatic">
      <parent>dummy_1</parent>
      <child>dummy_2</child>
      <axis>
        <xyz>1 0 0</xyz>
        <!-- Drake attaches an actuator to all and only joints with a nonzero
             effort limit. -->
        <limit>
          <lower>-0.5</lower>
          <upper>0.5</upper>
          <effort>10</effort>
        </limit>
        <dynamics>
          <damping>0.1</damping>
          <friction>0</friction>
        </dynamics>
      </axis>
    </joint>
    <joint name="y_joint" type="prismatic">
      <parent>dummy_2</parent>
      <child>dummy_3</child>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-0.8</lower>
          <upper>0</upper>
          <effort>10</effort>
        </limit>
        <dynamics>
          <damping>0.1</damping>
          <friction>0</friction>
        </dynamics>
      </axis>
    </joint>
    <joint name="theta_joint" type="revolute">
      <parent>dummy_3</parent>
      <child>paddle</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-3.14</lower>
          <upper>3.14</upper>
          <effort>10</effort>
        </limit>
        <dynamics>
          <damping>.01</damping>
          <friction>0</friction>
        </dynamics>
      </axis>
    </joint>    
  </model>
</sdf>
